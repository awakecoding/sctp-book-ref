.SUFFIXES: .o .c .cc .C .l .y .s$(OBJDIR)/%.s:     %.c	$(COMPILE.c) -S $<$(OBJDIR)/%.s:     %.cc	$(COMPILE.cc) -S $<$(OBJDIR)/%.o:     %.c	$(COMPILE.c) $<$(OBJDIR)/%.o:     %.cc	$(COMPILE.cc) $<$(OBJDIR)/%.o:     %.C	$(COMPILE.cc) $<# We always do that.# Comment it out if you don't want the auto dependency.# all:: depend